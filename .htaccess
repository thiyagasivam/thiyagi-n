RewriteEngine On

# Force www and HTTPS (handle both non-www and non-https)
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^thiyagi\.com$ [NC]
RewriteRule ^(.*)$ https://www.thiyagi.com/$1 [L,R=301]

# Set custom 404 error page
ErrorDocument 404 /404.php

# Sitemap rules
RewriteRule ^sitemap\.xml$ sitemap.php [L]
RewriteRule ^sitemap/?$ sitemap.php [L]
RewriteRule ^service-center-sitemap\.xml$ service-center-sitemap.php [L]

# Permanent redirect for renamed holiday files
RewriteRule ^albania-holidays-2025\.php$ /albania-holidays-2026.php [L,R=301]

# Prevent access to .htaccess and other sensitive files
<Files ".htaccess">
    Order allow,deny
    Deny from all
</Files>

# Block access to generated-pages directory (returns 410 Gone for removed content)
RewriteRule ^generated-pages/ - [L,R=410]

# Block non-existent pages and old URLs (returns 410 Gone for removed content)
# Time conversion pages with numbers
RewriteRule ^(weeks-to-days|months-to-weeks|days-to-weeks|weeks-to-months)/[0-9]+$ - [L,R=410]

# Volume/weight conversion pages with numbers  
RewriteRule ^(ounces-to-liters|gallons-to-liters|liters-to-gallons|liters-to-ounces|gallons-to-ounces|kg-to-lbs)/[0-9,]+$ - [L,R=410]

# Length conversion pages with numbers (includes numbers with commas)
RewriteRule ^(cm-to-feet-inches|feet-inches-to-cm|inches-to-feet|feet-to-inches|kph-to-mph)/[0-9,]+$ - [L,R=410]

# Non-existent conversion pages
RewriteRule ^(weeks-to-months|cups-to-ounces|ounces-to-cups|mps-to-mph|ounce-to-cup|cup-to-ounce|pint-to-cup|cooking-converter|speed-converter|ounces-to-tablespoons|cup-to-gallon|pint-to-quart|fluid-ounce-us-to-ml|volume-converters|tablespoons-to-teaspoons|teaspoons-to-ml|ppm-to-mg-l|mph-to-kmh|knot-to-kmh)$ - [L,R=410]

# Non-existent area/unit conversion pages
RewriteRule ^(cent-to-bigha|cent-to-hectare|square-mile-to-hectare|marla-to-kanal|square-meter-to-marla)$ - [L,R=410]

# Non-existent physics/chemistry conversion pages
RewriteRule ^(atmosphere-to-pascal|atm-to-psi|atm-to-bar|mmhg-to-bar|newtons-to-pounds|kg-to-grams|kg-to-pounds|pounds-to-kg|horsepower-to-watts|watts-to-horsepower|kw-to-watts|joule-to-btu|joule-to-cal|btu-to-cal|watt-to-btu-per-hour|gb-to-tb|tb-to-gb|nanograms-to-micrograms|micrograms-to-grams|mole-to-gram|radian-to-degree|kilometer-per-hour-to-meter-per-second)$ - [L,R=410]

# Non-existent calculator/tool pages
RewriteRule ^(youtube-views-ratio-calculator|seo-analyzer|image-compressor|flip-image|rotate-image|reverse-image-search|distance-calculator|cubic-meter-to-cubic-yard|cubic-yard-to-cubic-meter)$ - [L,R=410]

# Non-existent two-wheeler loan calculator paths
RewriteRule ^calculators/two-wheeler-loan/.+$ - [L,R=410]

# Non-existent NBFC calculator paths
RewriteRule ^calculators/(cashbean|payme|prefr|rupifi|money-club|nira|fairmoney|credflow|capfloat|finzy|moneyview|zest-money|creditbee|ring)-(personal|home)-loan-emi-calculator$ - [L,R=410]
RewriteRule ^calculators/nbfc/[^/]+/?$ - [L,R=410]

# Shop pages (entire shop section is non-existent)
RewriteRule ^shop/ - [L,R=410]
RewriteRule ^product/ - [L,R=410]

# Non-existent holiday pages
RewriteRule ^holiday/(france|northern-ireland|isle-of-man|guernsey)-holidays\.php$ - [L,R=410]
RewriteRule ^holiday/(nova-scotia|quebec|saskatchewan|british-columbia)-holidays\.php$ - [L,R=410]
