# Pincode pages
RewriteRule ^pincode/(quick-fix|api_test|ladakh|state-districts|pincode-detail)$ - [L,R=410]

# Blog WordPress paths (entire blog section)
RewriteRule ^blog/ - [L,R=410]
RewriteRule ^disclaimer/ - [L,R=410]
RewriteRule ^about-us-2/ - [L,R=410]
RewriteRule ^faq/ - [L,R=410]

# Gallery and shows
RewriteRule ^gallery/connections/$ - [L,R=410]
RewriteRule ^shows/hisaab-barabar/$ - [L,R=410]
RewriteRule ^elements/text-sliders/$ - [L,R=410]

# WordPress and RSS feeds
RewriteRule ^(rss\.xml|wp-content|wp-includes|wp-admin|xmlrpc)/ - [L,R=410]

# Apple app association files
RewriteRule ^(\.well-known/)?apple-app-site-association$ - [L,R=410]

# Old image paths
RewriteRule ^images/(tneb-calculator-preview|calculator-screenshot|emi-calculator-twitter|emi-calculator-og|wifi-password-generator|logo)\.jpg$ - [L,R=410]
RewriteRule ^images/logo\.png$ - [L,R=410]
RewriteRule ^wp-content/uploads/.*\.png$ - [L,R=410]

# Miscellaneous non-existent pages
RewriteRule ^[0-9]+\.htm$ - [L,R=410]

# Remove .php from URL (redirect to clean URL) - but skip directory-based URLs
RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s/([^.?\s]+)\.php [NC]
RewriteCond %1 !^(rto-details|electricity-board|service-center|holiday)/ [NC]
RewriteRule ^ /%1? [R=301,L]

# Directory-based URL rules (highest priority)
# Calculator
