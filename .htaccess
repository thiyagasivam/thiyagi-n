RewriteEngine On

# Force www and HTTPS (handle both non-www and non-https)
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^thiyagi\.com$ [NC]
RewriteRule ^(.*)$ https://www.thiyagi.com/$1 [L,R=301]

# Set custom 404 error page
ErrorDocument 404 /404.php

# Sitemap rule
RewriteRule ^sitemap\.xml$ sitemap.php [L]
RewriteRule ^sitemap/?$ sitemap.php [L]

# Prevent access to .htaccess and other sensitive files
<Files ".htaccess">
    Order allow,deny
    Deny from all
</Files>

# Block access to generated-pages directory (returns 410 Gone for removed content)
RewriteRule ^generated-pages/ - [L,R=410]

# Remove .php from URL (redirect to clean URL) - but skip directory-based URLs
RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s/([^.?\s]+)\.php [NC]
RewriteCond %1 !^(rto-details|electricity-board|service-center|holiday)/ [NC]
RewriteRule ^ /%1? [R=301,L]

# Directory-based URL rules (highest priority)
# Calculator
RewriteRule ^calculator/?$ /calculator/index.html [L]
RewriteRule ^calculator/banks/?$ /calculator/banks/index.php [L]

# Calculators (new directory)
RewriteRule ^calculators/?$ /calculators/index.php [L]

